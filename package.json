{
  "name": "enter-sandman",
  "version": "1.0.0",
  "description": "A backend JSON API for getting an overview on the most active users & individual users.",
  "main": "src/server.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec babel-node src/server.js",
    "build": "babel src --out-dir build",
    "serve": "NODE_ENV=production node --require @babel/polyfill build/server.js",
    "createIndex": "node test/createIndex.js",
    "createDB": "node test/createDB.js",
    "populateDB": "node test/populateData.js",
    "eslint": "eslint 'src/**/*.js' 'database/**/*.js'",
    "clean": "rm -rf build"
  },
  "keywords": [],
  "author": "Akash Sawant",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "pg": "^7.4.1",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "^1.2.0",
    "express-rate-limit": "^2.11.0",
    "helmet": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "nodemon": "^1.14.11"
  }
